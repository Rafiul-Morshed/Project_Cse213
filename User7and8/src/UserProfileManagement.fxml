      <TextField fx:id="currentUserIdFXID" layoutX="200.0" layoutY="200.0" promptText="Enter Name" text="Current User ID" />
      <Button layoutX="403.0" layoutY="243.0" mnemonicParsing="false" onAction="#updateButtonOnAction" text="Update" />
